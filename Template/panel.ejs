<div id="side-panel">
    <% messages.map( message => { %>
        <button class="message-btn">
            <div>
                <h4><%=message.user%></h4>
                <p><%=message.text%></p>
                <span><%=message.added%></span>
            </div>
        </button>
    <% }) %>
</div>